{
  "name": "launchedto.space-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "cd website && npm install && cd ../editor && npm install",
    "clean": "rm -rf dist && rm -rf node_modules && cd website && rm -rf node_modules && cd ../editor && rm -rf node_modules && rm -rf dist",
    "serve": "cd website && npm run serve",
    "build": "cd website && npm run build",
    "lint": "cd website && npm run lint",
    "editor": "cd editor && npm run dev",
    "thumb": "bash ./scripts/createThumbnails.sh"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1"
  }
}
